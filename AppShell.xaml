<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="YGOSearcher.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:YGOSearcher"
    xmlns:views="clr-namespace:YGOSearcher.View"
    Title="YGOSearcher">

    <TabBar>
        <ShellContent
        Title="Main Page"
        ContentTemplate="{DataTemplate views:MainPageView}"
        />
        <ShellContent
        Title="About"
        ContentTemplate="{DataTemplate views:AboutView}"
        />
    </TabBar>



</Shell>
